print ("ingrese la cantidad de plata")
Cantidad_de_plata= float(input())
print("ingrese cantidad de dolares")
Cantidad_de_dolares= float(input())
Cantidad_de_dolares= Cantidad_de_dolares/Cantidad_de_plata
print("con", Cantidad_de_plata, "pesos se podrian comprar", Cantidad_de_dolares, "cantidad de dolares")